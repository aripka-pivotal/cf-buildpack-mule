#!/usr/bin/env bash
# bin/release <build-dir>
#note the below uses a static path to JDK.  This is not wise.
cat << EOF
addons: 
config_vars:
default_process_types:
   web: MULE_HOME=/app/local/mule-standalone-3.3.0 JAVA_HOME=/app/local/jdk1.7.0_80 PATH=/app/local/jdk1.7.0_80/bin:/app/local/mule-standalone-3.5.0/bin:\$PATH mule -M-Dhttp.port=\$PORT
EOF
